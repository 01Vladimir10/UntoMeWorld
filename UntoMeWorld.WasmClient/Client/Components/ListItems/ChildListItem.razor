@using UntoMeWorld.Domain.Model
<ExpandableItem>
    <ItemTemplate>
        @Child.Name
    </ItemTemplate>
    
    <ExpandedContentTemplate>
        @Child.ToString()
    </ExpandedContentTemplate>
</ExpandableItem>

@code
{
    private Child Child => ListItem.Item;
    [Parameter] public ListItem<string, Child> ListItem { get; set; }
    
}
